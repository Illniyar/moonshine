{
  "name": "moonshine-js",
  "version": "0.0.6",
  "author": "Illniyar",
  "description": "a web framework for rapid development of SPA apps with angular.js,node and mongodb",

  "repository": {
    "type": "git",
    "url": "git://github.com/Illniyar/moonshine"
  },
  "licenses": {
    "type": "MIT",
    "url": "http://opensource.org/licenses/MIT"
  },
  "homepage": "https://github.com/Illniyar/moonshine",
  "keywords": [
    "framework",
    "spa",
    "mean",
    "django",
    "rapid"
  ],
  "main":"./src",
  "devDependencies": [
    "mocha",
	"superagent"
  ],
  "dependencies": {
      "express":"~3.1.0",
      "mongoose":"~3",
      "lograp":"0.0.x",
      "winston":"0.7.x",
      "baucis":"~0.6.22",
      "when":"~2.5.1",
      "fs-extra":"~0.6.0",
      "optimist":"~0.6.0",
      "require-like":"~0.1.0",
	  "async":"~0.2",
	  "lingo":"~0.0.5"
  },
  "bundledDependencies":[
    "moonshine-core",
    "moonshine-core-base",
    "moonshine-core-logging",
    "moonshine-core-persistence",
    "moonshine-core-server",
    "moonshine-core-api",
    "moonshine-core-static",
	"moon-contrib-angular"
  ],
  "bin": {
    "moonshine": "./bin/moonshine"
  },
  "scripts": {
    "test": "mocha --harmony-proxies --recursive --reporter Spec node_modules/moonshine*/test/*.spec.js test"
  }
}